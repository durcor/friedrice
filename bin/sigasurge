#!/bin/sh
rm -rf /tmp/sigasurge
sudo killall openvpn
mkdir /tmp/sigasurge

sudo openvpn l.ovpn &
sleep 5
echo $(curl -s -w '%{time_total}' -o /dev/null http://ddg.gg) >/tmp/sigasurge/l.txt
echo $(curl -s -w '%{time_total}' -o /dev/null http://ddg.gg) >>/tmp/sigasurge/l.txt
echo $(curl -s -w '%{time_total}' -o /dev/null http://ddg.gg) >>/tmp/sigasurge/l.txt
echo $(curl -s -w '%{time_total}' -o /dev/null http://ddg.gg) >>/tmp/sigasurge/l.txt
echo $(curl -s -w '%{time_total}' -o /dev/null http://ddg.gg) >>/tmp/sigasurge/l.txt
echo $(curl -s -w '%{time_total}' -o /dev/null http://ddg.gg) >>/tmp/sigasurge/l.txt
echo $(curl -s -w '%{time_total}' -o /dev/null http://ddg.gg) >>/tmp/sigasurge/l.txt
echo $(curl -s -w '%{time_total}' -o /dev/null http://ddg.gg) >>/tmp/sigasurge/l.txt
echo $(curl -s -w '%{time_total}' -o /dev/null http://ddg.gg) >>/tmp/sigasurge/l.txt
echo $(curl -s -w '%{time_total}' -o /dev/null http://ddg.gg) >>/tmp/sigasurge/l.txt
# for i in `seq 10`; do 'echo $(curl -s -w '%{time_total}' -o /dev/null http://ddg.gg) >>/tmp/sigasurge/l.txt' ; done
sudo killall openvpn

sudo openvpn n.ovpn &
sleep 5
echo $(curl -s -w '%{time_total}' -o /dev/null http://ddg.gg) >/tmp/sigasurge/n.txt
echo $(curl -s -w '%{time_total}' -o /dev/null http://ddg.gg) >>/tmp/sigasurge/n.txt
echo $(curl -s -w '%{time_total}' -o /dev/null http://ddg.gg) >>/tmp/sigasurge/n.txt
echo $(curl -s -w '%{time_total}' -o /dev/null http://ddg.gg) >>/tmp/sigasurge/n.txt
echo $(curl -s -w '%{time_total}' -o /dev/null http://ddg.gg) >>/tmp/sigasurge/n.txt
echo $(curl -s -w '%{time_total}' -o /dev/null http://ddg.gg) >>/tmp/sigasurge/n.txt
echo $(curl -s -w '%{time_total}' -o /dev/null http://ddg.gg) >>/tmp/sigasurge/n.txt
echo $(curl -s -w '%{time_total}' -o /dev/null http://ddg.gg) >>/tmp/sigasurge/n.txt
echo $(curl -s -w '%{time_total}' -o /dev/null http://ddg.gg) >>/tmp/sigasurge/n.txt
echo $(curl -s -w '%{time_total}' -o /dev/null http://ddg.gg) >>/tmp/sigasurge/n.txt
# for i in `seq 10`; do 'echo $(curl -s -w '%{time_total}' -o /dev/null http://ddg.gg) >>/tmp/sigasurge/n.txt' ; done
sudo killall openvpn

sudo openvpn r.ovpn &
sleep 5
echo $(curl -s -w '%{time_total}' -o /dev/null http://ddg.gg) >/tmp/sigasurge/r.txt
echo $(curl -s -w '%{time_total}' -o /dev/null http://ddg.gg) >>/tmp/sigasurge/r.txt
echo $(curl -s -w '%{time_total}' -o /dev/null http://ddg.gg) >>/tmp/sigasurge/r.txt
echo $(curl -s -w '%{time_total}' -o /dev/null http://ddg.gg) >>/tmp/sigasurge/r.txt
echo $(curl -s -w '%{time_total}' -o /dev/null http://ddg.gg) >>/tmp/sigasurge/r.txt
echo $(curl -s -w '%{time_total}' -o /dev/null http://ddg.gg) >>/tmp/sigasurge/r.txt
echo $(curl -s -w '%{time_total}' -o /dev/null http://ddg.gg) >>/tmp/sigasurge/r.txt
echo $(curl -s -w '%{time_total}' -o /dev/null http://ddg.gg) >>/tmp/sigasurge/r.txt
echo $(curl -s -w '%{time_total}' -o /dev/null http://ddg.gg) >>/tmp/sigasurge/r.txt
echo $(curl -s -w '%{time_total}' -o /dev/null http://ddg.gg) >>/tmp/sigasurge/r.txt
# for i in `seq 10`; do 'echo $(curl -s -w '%{time_total}' -o /dev/null http://ddg.gg) >>/tmp/sigasurge/r.txt' ; done
sudo killall openvpn

sudo openvpn s.ovpn &
sleep 5
echo $(curl -s -w '%{time_total}' -o /dev/null http://ddg.gg) >/tmp/sigasurge/s.txt
echo $(curl -s -w '%{time_total}' -o /dev/null http://ddg.gg) >>/tmp/sigasurge/s.txt
echo $(curl -s -w '%{time_total}' -o /dev/null http://ddg.gg) >>/tmp/sigasurge/s.txt
echo $(curl -s -w '%{time_total}' -o /dev/null http://ddg.gg) >>/tmp/sigasurge/s.txt
echo $(curl -s -w '%{time_total}' -o /dev/null http://ddg.gg) >>/tmp/sigasurge/s.txt
echo $(curl -s -w '%{time_total}' -o /dev/null http://ddg.gg) >>/tmp/sigasurge/s.txt
echo $(curl -s -w '%{time_total}' -o /dev/null http://ddg.gg) >>/tmp/sigasurge/s.txt
echo $(curl -s -w '%{time_total}' -o /dev/null http://ddg.gg) >>/tmp/sigasurge/s.txt
echo $(curl -s -w '%{time_total}' -o /dev/null http://ddg.gg) >>/tmp/sigasurge/s.txt
echo $(curl -s -w '%{time_total}' -o /dev/null http://ddg.gg) >>/tmp/sigasurge/s.txt
# for i in `seq 10`; do 'echo $(curl -s -w '%{time_total}' -o /dev/null http://ddg.gg) >>/tmp/sigasurge/s.txt' ; done
sudo killall openvpn

awk '{ total += ; count++ } END { print total/count}' l.txt
awk '{ total += ; count++ } END { print total/count}' n.txt
awk '{ total += ; count++ } END { print total/count}' r.txt
awk '{ total += ; count++ } END { print total/count}' s.txt
