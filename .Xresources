! ~/.Xresources
! vi:ft=xdefaults

! X colors.
*foreground:        #ffe2f7
*background:        #0A1815
*.foreground:       #ffe2f7
*.background:       #0A1815
emacs*foreground:   #ffe2f7
emacs*background:   #0A1815
URxvt*foreground:   #ffe2f7
XTerm*foreground:   #ffe2f7
UXTerm*foreground:  #ffe2f7
URxvt*background:   [100]#0A1815
XTerm*background:   #0A1815
UXTerm*background:  #0A1815
URxvt*cursorColor:  #ffe2f7
XTerm*cursorColor:  #ffe2f7
UXTerm*cursorColor: #ffe2f7
URxvt*borderColor:  [100]#0A1815

! Colors 0-15.
*.color0: #0A1815
*color0:  #0A1815
*.color1: #DB5E2E
*color1:  #DB5E2E
*.color2: #9D572D
*color2:  #9D572D
*.color3: #E4A550
*color3:  #E4A550
*.color4: #4C4546
*color4:  #4C4546
*.color5: #CD6746
*color5:  #CD6746
*.color6: #98391F
*color6:  #98391F
*.color7: #d0acbc
*color7:  #d0acbc
*.color8: #15342d
*color8:  #15342d
*.color9: #ff722a
*color9:  #ff722a
*.color10: #d96d2d
*color10:  #d96d2d
*.color11: #ffd953
*color11:  #ffd953
*.color12: #6c5054
*color12:  #6c5054
*.color13: #ff7c49
*color13:  #ff7c49
*.color14: #d1431c
*color14:  #d1431c
*.color15: #ffe2f7
*color15:  #ffe2f7

! Black color that will not be affected by bold highlighting.
*.color66: #0A1815
*color66:  #0A1815

! Xclock colors.
XClock*foreground: #ffe2f7
XClock*background: #0A1815
! XClock*majorColor:  rgba:ff/f8/ff/ff
! XClock*minorColor:  rgba:ff/f8/ff/ff
! XClock*hourColor:   rgba:ff/f8/ff/ff
! XClock*minuteColor: rgba:ff/f8/ff/ff
! XClock*secondColor: rgba:ff/f8/ff/ff

! Set depth to make transparency work.
URxvt*depth: 32

*.def-font: Inconsolata Nerd Font
*def-font: Inconsolata Nerd Font
