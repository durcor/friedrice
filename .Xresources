! ~/.Xresources
! vi:ft=xdefaults

! X colors.
*foreground:        #fffff7
*background:        #0D0E12
*.foreground:       #fffff7
*.background:       #0D0E12
emacs*foreground:   #fffff7
emacs*background:   #0D0E12
URxvt*foreground:   #fffff7
XTerm*foreground:   #fffff7
UXTerm*foreground:  #fffff7
URxvt*background:   [100]#0D0E12
XTerm*background:   #0D0E12
UXTerm*background:  #0D0E12
URxvt*cursorColor:  #fffff7
XTerm*cursorColor:  #fffff7
UXTerm*cursorColor: #fffff7
URxvt*borderColor:  [100]#0D0E12

! Colors 0-15.
*.color0: #0D0E12
*color0:  #0D0E12
*.color1: #8A776D
*color1:  #8A776D
*.color2: #6F6966
*color2:  #6F6966
*.color3: #C6B8A8
*color3:  #C6B8A8
*.color4: #636582
*color4:  #636582
*.color5: #A69F94
*color5:  #A69F94
*.color6: #8F867B
*color6:  #8F867B
*.color7: #dfdbd2
*color7:  #dfdbd2
*.color8: #1d1f29
*color8:  #1d1f29
*.color9: #c3957d
*color9:  #c3957d
*.color10: #9e8376
*color10:  #9e8376
*.color11: #fff0c2
*color11:  #fff0c2
*.color12: #7176b7
*color12:  #7176b7
*.color13: #ecd3ac
*color13:  #ecd3ac
*.color14: #cbaf8e
*color14:  #cbaf8e
*.color15: #fffff7
*color15:  #fffff7

! Black color that will not be affected by bold highlighting.
*.color66: #0D0E12
*color66:  #0D0E12

! Xclock colors.
XClock*foreground: #fffff7
XClock*background: #0D0E12
! XClock*majorColor:  rgba:ff/f8/ff/ff
! XClock*minorColor:  rgba:ff/f8/ff/ff
! XClock*hourColor:   rgba:ff/f8/ff/ff
! XClock*minuteColor: rgba:ff/f8/ff/ff
! XClock*secondColor: rgba:ff/f8/ff/ff

! Set depth to make transparency work.
URxvt*depth: 32

*.def-font: Inconsolata Nerd Font
*def-font: Inconsolata Nerd Font
