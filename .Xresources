! ~/.Xresources
! vi:ft=xdefaults

! X colors.
*foreground:        #fdffff
*background:        #0c090a
*.foreground:       #fdffff
*.background:       #0c090a
emacs*foreground:   #fdffff
emacs*background:   #0c090a
URxvt*foreground:   #fdffff
XTerm*foreground:   #fdffff
UXTerm*foreground:  #fdffff
URxvt*background:   [100]#0c090a
XTerm*background:   #0c090a
UXTerm*background:  #0c090a
URxvt*cursorColor:  #fdffff
XTerm*cursorColor:  #fdffff
UXTerm*cursorColor: #fdffff
URxvt*borderColor:  [100]#0c090a

! Colors 0-15.
*.color0: #0c090a
*color0:  #0c090a
*.color1: #9A6846
*color1:  #9A6846
*.color2: #6E6E71
*color2:  #6E6E71
*.color3: #9E8F73
*color3:  #9E8F73
*.color4: #767B84
*color4:  #767B84
*.color5: #936969
*color5:  #936969
*.color6: #788A95
*color6:  #788A95
*.color7: #c3c5c5
*color7:  #c3c5c5
*.color8: #23191c
*color8:  #23191c
*.color9: #d6844c
*color9:  #d6844c
*.color10: #8080a1
*color10:  #8080a1
*.color11: #dfbf83
*color11:  #dfbf83
*.color12: #899bbb
*color12:  #899bbb
*.color13: #cf7878
*color13:  #cf7878
*.color14: #8ab7d3
*color14:  #8ab7d3
*.color15: #fdffff
*color15:  #fdffff

! Black color that will not be affected by bold highlighting.
*.color66: #0c090a
*color66:  #0c090a

! Xclock colors.
XClock*foreground: #fdffff
XClock*background: #0c090a
! XClock*majorColor:  rgba:ff/f8/ff/ff
! XClock*minorColor:  rgba:ff/f8/ff/ff
! XClock*hourColor:   rgba:ff/f8/ff/ff
! XClock*minuteColor: rgba:ff/f8/ff/ff
! XClock*secondColor: rgba:ff/f8/ff/ff

! Set depth to make transparency work.
URxvt*depth: 32

*.def-font: Inconsolata Nerd Font
*def-font: Inconsolata Nerd Font
