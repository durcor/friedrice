! ~/.Xresources
! vi:ft=xdefaults

! X colors.
*foreground:        #ffffec
*background:        #1b180d
*.foreground:       #ffffec
*.background:       #1b180d
emacs*foreground:   #ffffec
emacs*background:   #1b180d
URxvt*foreground:   #ffffec
XTerm*foreground:   #ffffec
UXTerm*foreground:  #ffffec
URxvt*background:   [100]#1b180d
XTerm*background:   #1b180d
UXTerm*background:  #1b180d
URxvt*cursorColor:  #ffffec
XTerm*cursorColor:  #ffffec
UXTerm*cursorColor: #ffffec
URxvt*borderColor:  [100]#1b180d

! Colors 0-15.
*.color0: #1b180d
*color0:  #1b180d
*.color1: #8C764F
*color1:  #8C764F
*.color2: #78835E
*color2:  #78835E
*.color3: #CCA86B
*color3:  #CCA86B
*.color4: #A59164
*color4:  #A59164
*.color5: #AEA196
*color5:  #AEA196
*.color6: #CEB693
*color6:  #CEB693
*.color7: #ede3cd
*color7:  #ede3cd
*.color8: #363019
*color8:  #363019
*.color9: #c49d58
*color9:  #c49d58
*.color10: #a1b86b
*color10:  #a1b86b
*.color11: #ffdf76
*color11:  #ffdf76
*.color12: #e7c370
*color12:  #e7c370
*.color13: #f7cfae
*color13:  #f7cfae
*.color14: #fff0a8
*color14:  #fff0a8
*.color15: #ffffec
*color15:  #ffffec

! Black color that will not be affected by bold highlighting.
*.color66: #1b180d
*color66:  #1b180d

! Xclock colors.
XClock*foreground: #ffffec
XClock*background: #1b180d
! XClock*majorColor:  rgba:ff/f8/ff/ff
! XClock*minorColor:  rgba:ff/f8/ff/ff
! XClock*hourColor:   rgba:ff/f8/ff/ff
! XClock*minuteColor: rgba:ff/f8/ff/ff
! XClock*secondColor: rgba:ff/f8/ff/ff

! Set depth to make transparency work.
URxvt*depth: 32

*.def-font: Inconsolata Nerd Font
*def-font: Inconsolata Nerd Font
