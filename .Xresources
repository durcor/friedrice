! ~/.Xresources
! vi:ft=xdefaults

! X colors.
*foreground:        #dfffff
*background:        #16191c
*.foreground:       #dfffff
*.background:       #16191c
emacs*foreground:   #dfffff
emacs*background:   #16191c
URxvt*foreground:   #dfffff
XTerm*foreground:   #dfffff
UXTerm*foreground:  #dfffff
URxvt*background:   [100]#16191c
XTerm*background:   #16191c
UXTerm*background:  #16191c
URxvt*cursorColor:  #dfffff
XTerm*cursorColor:  #dfffff
UXTerm*cursorColor: #dfffff
URxvt*borderColor:  [100]#16191c

! Colors 0-15.
*.color0: #16191c
*color0:  #16191c
*.color1: #5B93A9
*color1:  #5B93A9
*.color2: #45788F
*color2:  #45788F
*.color3: #F1992B
*color3:  #F1992B
*.color4: #3C6D85
*color4:  #3C6D85
*.color5: #9D7A88
*color5:  #9D7A88
*.color6: #5491AB
*color6:  #5491AB
*.color7: #abced9
*color7:  #abced9
*.color8: #272c32
*color8:  #272c32
*.color9: #65c6ec
*color9:  #65c6ec
*.color10: #4ba1c7
*color10:  #4ba1c7
*.color11: #ffc825
*color11:  #ffc825
*.color12: #4192b9
*color12:  #4192b9
*.color13: #de8cad
*color13:  #de8cad
*.color14: #5cc3ee
*color14:  #5cc3ee
*.color15: #dfffff
*color15:  #dfffff

! Black color that will not be affected by bold highlighting.
*.color66: #16191c
*color66:  #16191c

! Xclock colors.
XClock*foreground: #dfffff
XClock*background: #16191c
! XClock*majorColor:  rgba:ff/f8/ff/ff
! XClock*minorColor:  rgba:ff/f8/ff/ff
! XClock*hourColor:   rgba:ff/f8/ff/ff
! XClock*minuteColor: rgba:ff/f8/ff/ff
! XClock*secondColor: rgba:ff/f8/ff/ff

! Set depth to make transparency work.
URxvt*depth: 32

*.def-font: Inconsolata Nerd Font
*def-font: Inconsolata Nerd Font
