! ~/.Xresources
! vi:ft=xdefaults

! X colors.
*foreground:        #d3ecff
*background:        #0a0a0b
*.foreground:       #d3ecff
*.background:       #0a0a0b
emacs*foreground:   #d3ecff
emacs*background:   #0a0a0b
URxvt*foreground:   #d3ecff
XTerm*foreground:   #d3ecff
UXTerm*foreground:  #d3ecff
URxvt*background:   [100]#0a0a0b
XTerm*background:   #0a0a0b
UXTerm*background:  #0a0a0b
URxvt*cursorColor:  #d3ecff
XTerm*cursorColor:  #d3ecff
UXTerm*cursorColor: #d3ecff
URxvt*borderColor:  [100]#0a0a0b

! Colors 0-15.
*.color0: #0a0a0b
*color0:  #0a0a0b
*.color1: #4863A0
*color1:  #4863A0
*.color2: #3A5694
*color2:  #3A5694
*.color3: #879F75
*color3:  #879F75
*.color4: #364999
*color4:  #364999
*.color5: #77889B
*color5:  #77889B
*.color6: #4F74CE
*color6:  #4F74CE
*.color7: #a8bced
*color7:  #a8bced
*.color8: #212125
*color8:  #212125
*.color9: #4e7adf
*color9:  #4e7adf
*.color10: #3d6acd
*color10:  #3d6acd
*.color11: #ace086
*color11:  #ace086
*.color12: #3856d4
*color12:  #3856d4
*.color13: #88afdb
*color13:  #88afdb
*.color14: #548fff
*color14:  #548fff
*.color15: #d3ecff
*color15:  #d3ecff

! Black color that will not be affected by bold highlighting.
*.color66: #0a0a0b
*color66:  #0a0a0b

! Xclock colors.
XClock*foreground: #d3ecff
XClock*background: #0a0a0b
! XClock*majorColor:  rgba:ff/f8/ff/ff
! XClock*minorColor:  rgba:ff/f8/ff/ff
! XClock*hourColor:   rgba:ff/f8/ff/ff
! XClock*minuteColor: rgba:ff/f8/ff/ff
! XClock*secondColor: rgba:ff/f8/ff/ff

! Set depth to make transparency work.
URxvt*depth: 32

*.def-font: Fantasque Sans Mono Nerd Font
*def-font: Fantasque Sans Mono Nerd Font
