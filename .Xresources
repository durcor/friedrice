! ~/.Xresources
! vi:ft=xdefaults

! X colors.
*foreground:        #ebffff
*background:        #19060f
*.foreground:       #ebffff
*.background:       #19060f
emacs*foreground:   #ebffff
emacs*background:   #19060f
URxvt*foreground:   #ebffff
XTerm*foreground:   #ebffff
UXTerm*foreground:  #ebffff
URxvt*background:   [100]#19060f
XTerm*background:   #19060f
UXTerm*background:  #19060f
URxvt*cursorColor:  #ebffff
XTerm*cursorColor:  #ebffff
UXTerm*cursorColor: #ebffff
URxvt*borderColor:  [100]#19060f

! Colors 0-15.
*.color0: #19060f
*color0:  #19060f
*.color1: #689CA1
*color1:  #689CA1
*.color2: #009696
*color2:  #009696
*.color3: #CCB0AA
*color3:  #CCB0AA
*.color4: #2D8C90
*color4:  #2D8C90
*.color5: #EEA3DC
*color5:  #EEA3DC
*.color6: #00A1A1
*color6:  #00A1A1
*.color7: #ccebed
*color7:  #ccebed
*.color8: #420e27
*color8:  #420e27
*.color9: #76d8e2
*color9:  #76d8e2
*.color10: #01c1c2
*color10:  #01c1c2
*.color11: #ffd5c4
*color11:  #ffd5c4
*.color12: #2ec1c7
*color12:  #2ec1c7
*.color13: #ffb5ff
*color13:  #ffb5ff
*.color14: #01d0d0
*color14:  #01d0d0
*.color15: #ebffff
*color15:  #ebffff

! Black color that will not be affected by bold highlighting.
*.color66: #19060f
*color66:  #19060f

! Xclock colors.
XClock*foreground: #ebffff
XClock*background: #19060f
! XClock*majorColor:  rgba:ff/f8/ff/ff
! XClock*minorColor:  rgba:ff/f8/ff/ff
! XClock*hourColor:   rgba:ff/f8/ff/ff
! XClock*minuteColor: rgba:ff/f8/ff/ff
! XClock*secondColor: rgba:ff/f8/ff/ff

! Set depth to make transparency work.
URxvt*depth: 32

*.def-font: Fantasque Sans Mono Nerd Font
*def-font: Fantasque Sans Mono Nerd Font
