! ~/.Xresources
! vi:ft=xdefaults

! X colors.
*foreground:        #fffffa
*background:        #0E0F08
*.foreground:       #fffffa
*.background:       #0E0F08
emacs*foreground:   #fffffa
emacs*background:   #0E0F08
URxvt*foreground:   #fffffa
XTerm*foreground:   #fffffa
UXTerm*foreground:  #fffffa
URxvt*background:   [100]#0E0F08
XTerm*background:   #0E0F08
UXTerm*background:  #0E0F08
URxvt*cursorColor:  #fffffa
XTerm*cursorColor:  #fffffa
UXTerm*cursorColor: #fffffa
URxvt*borderColor:  [100]#0E0F08

! Colors 0-15.
*.color0: #0E0F08
*color0:  #0E0F08
*.color1: #C9594B
*color1:  #C9594B
*.color2: #99916E
*color2:  #99916E
*.color3: #E49766
*color3:  #E49766
*.color4: #A5A698
*color4:  #A5A698
*.color5: #DBAC9A
*color5:  #DBAC9A
*.color6: #6C8890
*color6:  #6C8890
*.color7: #dadcd5
*color7:  #dadcd5
*.color8: #262915
*color8:  #262915
*.color9: #ff654f
*color9:  #ff654f
*.color10: #d7c77d
*color10:  #d7c77d
*.color11: #ffbf6f
*color11:  #ffbf6f
*.color12: #e8ecb1
*color12:  #e8ecb1
*.color13: #ffd4b0
*color13:  #ffd4b0
*.color14: #7cb9cb
*color14:  #7cb9cb
*.color15: #fffffa
*color15:  #fffffa

! Black color that will not be affected by bold highlighting.
*.color66: #0E0F08
*color66:  #0E0F08

! Xclock colors.
XClock*foreground: #fffffa
XClock*background: #0E0F08
! XClock*majorColor:  rgba:ff/f8/ff/ff
! XClock*minorColor:  rgba:ff/f8/ff/ff
! XClock*hourColor:   rgba:ff/f8/ff/ff
! XClock*minuteColor: rgba:ff/f8/ff/ff
! XClock*secondColor: rgba:ff/f8/ff/ff

! Set depth to make transparency work.
URxvt*depth: 32

*.def-font: Inconsolata Nerd Font
*def-font: Inconsolata Nerd Font
