! ~/.Xresources
! vi:ft=xdefaults

! X colors.
*foreground:        #ffeeea
*background:        #040B2F
*.foreground:       #ffeeea
*.background:       #040B2F
emacs*foreground:   #ffeeea
emacs*background:   #040B2F
URxvt*foreground:   #ffeeea
XTerm*foreground:   #ffeeea
UXTerm*foreground:  #ffeeea
URxvt*background:   [100]#040B2F
XTerm*background:   #040B2F
UXTerm*background:  #040B2F
URxvt*cursorColor:  #ffeeea
XTerm*cursorColor:  #ffeeea
UXTerm*cursorColor: #ffeeea
URxvt*borderColor:  [100]#040B2F

! Colors 0-15.
*.color0: #040B2F
*color0:  #040B2F
*.color1: #9C6E8A
*color1:  #9C6E8A
*.color2: #5E678C
*color2:  #5E678C
*.color3: #A29BA1
*color3:  #A29BA1
*.color4: #2A548C
*color4:  #2A548C
*.color5: #DC6C87
*color5:  #DC6C87
*.color6: #678FA5
*color6:  #678FA5
*.color7: #e5c6c3
*color7:  #e5c6c3
*.color8: #07155d
*color8:  #07155d
*.color9: #dc7db7
*color9:  #dc7db7
*.color10: #6b7cc5
*color10:  #6b7cc5
*.color11: #e7b4df
*color11:  #e7b4df
*.color12: #2a6bc1
*color12:  #2a6bc1
*.color13: #ff77a4
*color13:  #ff77a4
*.color14: #74bee7
*color14:  #74bee7
*.color15: #ffeeea
*color15:  #ffeeea

! Black color that will not be affected by bold highlighting.
*.color66: #040B2F
*color66:  #040B2F

! Xclock colors.
XClock*foreground: #ffeeea
XClock*background: #040B2F
! XClock*majorColor:  rgba:ff/f8/ff/ff
! XClock*minorColor:  rgba:ff/f8/ff/ff
! XClock*hourColor:   rgba:ff/f8/ff/ff
! XClock*minuteColor: rgba:ff/f8/ff/ff
! XClock*secondColor: rgba:ff/f8/ff/ff

! Set depth to make transparency work.
URxvt*depth: 32

*.def-font: Fantasque Sans Mono Nerd Font
*def-font: Fantasque Sans Mono Nerd Font
