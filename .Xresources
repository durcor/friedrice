! ~/.Xresources
! vi:ft=xdefaults

! X colors.
*foreground:        #e7f4fd
*background:        #110d04
*.foreground:       #e7f4fd
*.background:       #110d04
emacs*foreground:   #e7f4fd
emacs*background:   #110d04
URxvt*foreground:   #e7f4fd
XTerm*foreground:   #e7f4fd
UXTerm*foreground:  #e7f4fd
URxvt*background:   [100]#110d04
XTerm*background:   #110d04
UXTerm*background:  #110d04
URxvt*cursorColor:  #e7f4fd
XTerm*cursorColor:  #e7f4fd
UXTerm*cursorColor: #e7f4fd
URxvt*borderColor:  [100]#110d04

! Colors 0-15.
*.color0: #110d04
*color0:  #110d04
*.color1: #B66024
*color1:  #B66024
*.color2: #4C4953
*color2:  #4C4953
*.color3: #CF9974
*color3:  #CF9974
*.color4: #5E5D6A
*color4:  #5E5D6A
*.color5: #B38974
*color5:  #B38974
*.color6: #9B736A
*color6:  #9B736A
*.color7: #aeb8bf
*color7:  #aeb8bf
*.color8: #3b2c0b
*color8:  #3b2c0b
*.color9: #fa7a20
*color9:  #fa7a20
*.color10: #5e5475
*color10:  #5e5475
*.color11: #ffc281
*color11:  #ffc281
*.color12: #6f6c96
*color12:  #6f6c96
*.color13: #fbab83
*color13:  #fbab83
*.color14: #da8a78
*color14:  #da8a78
*.color15: #e7f4fd
*color15:  #e7f4fd

! Black color that will not be affected by bold highlighting.
*.color66: #110d04
*color66:  #110d04

! Xclock colors.
XClock*foreground: #e7f4fd
XClock*background: #110d04
! XClock*majorColor:  rgba:ff/f8/ff/ff
! XClock*minorColor:  rgba:ff/f8/ff/ff
! XClock*hourColor:   rgba:ff/f8/ff/ff
! XClock*minuteColor: rgba:ff/f8/ff/ff
! XClock*secondColor: rgba:ff/f8/ff/ff

! Set depth to make transparency work.
URxvt*depth: 32

*.def-font: Fantasque Sans Mono Nerd Font
*def-font: Fantasque Sans Mono Nerd Font
