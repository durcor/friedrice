! ~/.Xresources
! vi:ft=xdefaults

! X colors.
*foreground:        #fff1fb
*background:        #15140d
*.foreground:       #fff1fb
*.background:       #15140d
emacs*foreground:   #fff1fb
emacs*background:   #15140d
URxvt*foreground:   #fff1fb
XTerm*foreground:   #fff1fb
UXTerm*foreground:  #fff1fb
URxvt*background:   [100]#15140d
XTerm*background:   #15140d
UXTerm*background:  #15140d
URxvt*cursorColor:  #fff1fb
XTerm*cursorColor:  #fff1fb
UXTerm*cursorColor: #fff1fb
URxvt*borderColor:  [100]#15140d

! Colors 0-15.
*.color0: #15140d
*color0:  #15140d
*.color1: #EA5C2E
*color1:  #EA5C2E
*.color2: #9D5F5B
*color2:  #9D5F5B
*.color3: #D69E58
*color3:  #D69E58
*.color4: #CD4D4A
*color4:  #CD4D4A
*.color5: #C0659E
*color5:  #C0659E
*.color6: #6A85C2
*color6:  #6A85C2
*.color7: #e1cbd3
*color7:  #e1cbd3
*.color8: #2e2b1c
*color8:  #2e2b1c
*.color9: #ff6e29
*color9:  #ff6e29
*.color10: #dc6d66
*color10:  #dc6d66
*.color11: #ffcf5e
*color11:  #ffcf5e
*.color12: #ff524e
*color12:  #ff524e
*.color13: #ff70d2
*color13:  #ff70d2
*.color14: #76a5ff
*color14:  #76a5ff
*.color15: #fff1fb
*color15:  #fff1fb

! Black color that will not be affected by bold highlighting.
*.color66: #15140d
*color66:  #15140d

! Xclock colors.
XClock*foreground: #fff1fb
XClock*background: #15140d
! XClock*majorColor:  rgba:ff/f8/ff/ff
! XClock*minorColor:  rgba:ff/f8/ff/ff
! XClock*hourColor:   rgba:ff/f8/ff/ff
! XClock*minuteColor: rgba:ff/f8/ff/ff
! XClock*secondColor: rgba:ff/f8/ff/ff

! Set depth to make transparency work.
URxvt*depth: 32

*.def-font: Inconsolata Nerd Font
*def-font: Inconsolata Nerd Font
