! ~/.Xresources
! vi:ft=xdefaults

! X colors.
*foreground:        #c2fffb
*background:        #1a1214
*.foreground:       #c2fffb
*.background:       #1a1214
emacs*foreground:   #c2fffb
emacs*background:   #1a1214
URxvt*foreground:   #c2fffb
XTerm*foreground:   #c2fffb
UXTerm*foreground:  #c2fffb
URxvt*background:   [100]#1a1214
XTerm*background:   #1a1214
UXTerm*background:  #1a1214
URxvt*cursorColor:  #c2fffb
XTerm*cursorColor:  #c2fffb
UXTerm*cursorColor: #c2fffb
URxvt*borderColor:  [100]#1a1214

! Colors 0-15.
*.color0: #1a1214
*color0:  #1a1214
*.color1: #C61E70
*color1:  #C61E70
*.color2: #0A837D
*color2:  #0A837D
*.color3: #919976
*color3:  #919976
*.color4: #D32A72
*color4:  #D32A72
*.color5: #E83479
*color5:  #E83479
*.color6: #678E77
*color6:  #678E77
*.color7: #8fc0b9
*color7:  #8fc0b9
*.color8: #37262a
*color8:  #37262a
*.color9: #ff1891
*color9:  #ff1891
*.color10: #03b3aa
*color10:  #03b3aa
*.color11: #c6d887
*color11:  #c6d887
*.color12: #ff2691
*color12:  #ff2691
*.color13: #ff3198
*color13:  #ff3198
*.color14: #76c897
*color14:  #76c897
*.color15: #c2fffb
*color15:  #c2fffb

! Black color that will not be affected by bold highlighting.
*.color66: #1a1214
*color66:  #1a1214

! Xclock colors.
XClock*foreground: #c2fffb
XClock*background: #1a1214
! XClock*majorColor:  rgba:ff/f8/ff/ff
! XClock*minorColor:  rgba:ff/f8/ff/ff
! XClock*hourColor:   rgba:ff/f8/ff/ff
! XClock*minuteColor: rgba:ff/f8/ff/ff
! XClock*secondColor: rgba:ff/f8/ff/ff

! Set depth to make transparency work.
URxvt*depth: 32

*.def-font: Fantasque Sans Mono Nerd Font
*def-font: Fantasque Sans Mono Nerd Font
