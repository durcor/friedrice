! ~/.Xresources
! vi:ft=xdefaults

! X colors.
*foreground:        #c9ffff
*background:        #0f0a0b
*.foreground:       #c9ffff
*.background:       #0f0a0b
emacs*foreground:   #c9ffff
emacs*background:   #0f0a0b
URxvt*foreground:   #c9ffff
XTerm*foreground:   #c9ffff
UXTerm*foreground:  #c9ffff
URxvt*background:   [100]#0f0a0b
XTerm*background:   #0f0a0b
UXTerm*background:  #0f0a0b
URxvt*cursorColor:  #c9ffff
XTerm*cursorColor:  #c9ffff
UXTerm*cursorColor: #c9ffff
URxvt*borderColor:  [100]#0f0a0b

! Colors 0-15.
*.color0: #0f0a0b
*color0:  #0f0a0b
*.color1: #B05E40
*color1:  #B05E40
*.color2: #5D8F6D
*color2:  #5D8F6D
*.color3: #DCAA3A
*color3:  #DCAA3A
*.color4: #276792
*color4:  #276792
*.color5: #5A565E
*color5:  #5A565E
*.color6: #546F92
*color6:  #546F92
*.color7: #97c7d2
*color7:  #97c7d2
*.color8: #27191c
*color8:  #27191c
*.color9: #f47243
*color9:  #f47243
*.color10: #69c988
*color10:  #69c988
*.color11: #ffe339
*color11:  #ffe339
*.color12: #2688c9
*color12:  #2688c9
*.color13: #746485
*color13:  #746485
*.color14: #5e8ecc
*color14:  #5e8ecc
*.color15: #c9ffff
*color15:  #c9ffff

! Black color that will not be affected by bold highlighting.
*.color66: #0f0a0b
*color66:  #0f0a0b

! Xclock colors.
XClock*foreground: #c9ffff
XClock*background: #0f0a0b
! XClock*majorColor:  rgba:ff/f8/ff/ff
! XClock*minorColor:  rgba:ff/f8/ff/ff
! XClock*hourColor:   rgba:ff/f8/ff/ff
! XClock*minuteColor: rgba:ff/f8/ff/ff
! XClock*secondColor: rgba:ff/f8/ff/ff

! Set depth to make transparency work.
URxvt*depth: 32

*.def-font: Inconsolata Nerd Font
*def-font: Inconsolata Nerd Font
