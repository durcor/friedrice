[locale]
    timeformat = %H:%M
    dateformat = %m/%d/%Y
    longdateformat = %m/%d/%Y
    datetimeformat = %m/%d/%Y %H:%M
    longdatetimeformat = %m/%d/%Y %H:%M

[default]
    show_all_days = True
    timedelta = 7d

[keybindings]
    delete = x
    down = j
    duplicate = y
    export = ctrl e
    external_edit = E
    left = h
    log = L
    mark = V
    new = o
    other = G
    quit = q
    right = l
    save = ctrl s
    search = /
    today = g
    up = k
    view = v
