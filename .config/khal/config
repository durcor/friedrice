[calendars]

    [[event]]
        path = ~/cal/event/
        color = 12

    [[asn]]
        path = ~/cal/asn/
        color = 10

    [[done]]
        path = ~/cal/done/
        color = 1

    [[birthdays]]
        path = ~/addbk/contacts/
        type = birthdays
        color = 11

    [[f1]]
        path = ~/cal/f1/
        color = 13

    [[indycar]]
