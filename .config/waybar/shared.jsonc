{
  "hyprland/submap": {
    "format": " {} ",
    "tooltip": false
  },
  "sway/mode": {
    "format": " {} ",
    "tooltip": false
  },

  "hyprland/window": {
    "format": "{}",
    "max-length": 30,
    "tooltip": false
  },

  "hyprland/workspaces": {
    "disable-scroll-wraparound": true,
    "smooth-scrolling-threshold": 4,
    "enable-bar-scroll": true,
    "format": "{name}"
  },

  "clock#time": {
    "interval": 1,
    "format": "{:%H:%M/%S}",
    "tooltip": false
  },

  "clock#date": {
    "interval": 20,
    "format": "{:%e %b %Y}",
    "tooltip": false
    // "tooltip-format": "{:%e %B %Y}"
  },

  "hyprland/language": {
    "format": "ÔÑú  {flag}",
    "min-length": 6,
    "on-click": "swaymsg 'input * xkb_switch_layout next'",
    "tooltip": false
  },

  "sway/language": {
    "format": "ÔÑú  {flag}",
    "min-length": 6,
    "on-click": "swaymsg 'input * xkb_switch_layout next'",
    "tooltip": false
  },

  "mpris": {
    "format": "{player_icon} {dynamic}",
    "format-paused": "{status_icon} <i>{dynamic}</i>",
    "player-icons": {
      "default": "‚ñ∂",
      "mpv": "üéµ"
    },
    "status-icons": {
      "paused": "‚è∏"
    },
  },

  // TODO: Support accuweather backend
  // TODO: Allow ability to easily configure multiple locations
  // TODO: Grab location using GPS. Fallback to IP. Override with config
  "custom/weather": {
      "exec": "wttrbar --nerd --fahrenheit --mph --custom-indicator '{ICON}  {temp_F}F {areaName}' --location 'San Diego'",
      "format": "{}",
      "tooltip": true,
      "interval": 3600,
      "return-type": "json"
  },

  "battery": {
    "interval": 10,
    "states": {
      "warning": 30,
      "critical": 15
    },
    "format-time": "{H}:{M:02}",
    "format": "{icon}  {capacity}% ({time})",
    "format-charging": "ÔÉß  {capacity}% ({time})",
    "format-charging-full": "ÔÉß  {capacity}%",
    "format-full": "{icon}  {capacity}%",
    "format-alt": "{icon}  {power}W",
    "format-icons": [
      "ÔâÑ",
      "ÔâÉ",
      "ÔâÇ",
      "ÔâÅ",
      "ÔâÄ"
    ],
    "tooltip": false
  },

  "tray": {
    "icon-size": 15,
    "spacing": 10
  },

  // FIXME: Show icon/battery level per device connected
  "bluetooth": {
    "format": "Ôäî {num_connections}",
    // "format-connected": "Ôäî {device_alias}",
    "format-connected-battery": "Ôäî {num_connections}: {icon} ({device_battery_percentage}%)",
    // "format-device-preference": [ "device1", "device2" ], // preference list deciding the displayed device
    "format-icons": {
      "headphone": "ÔÄ•",
      "hands-free": "ÔÄ•",
      "headset": "ÔÄ•",
      "phone": "ÔÇï",
      "portable": "ÔÇï",
      "car": "ÔÜπ",
      "default": "Ôäî"
    },
    "tooltip-format": "{controller_alias}\t{controller_address}\n\n{num_connections} connected",
    "tooltip-format-connected": "{controller_alias}\t{controller_address}\n\n{num_connections} connected\n\n{device_enumerate}",
    "tooltip-format-enumerate-connected": "{device_alias}\t{device_address}",
    "tooltip-format-enumerate-connected-battery": "{device_alias}\t{device_address}\t{device_battery_percentage}%",
    "on-click": "bt-select"
  },

  // "exec": "~/.local/bin/i3blocks/amdgpu"
  "custom/gpu": {
    "exec": "cat /sys/class/hwmon/hwmon1/device/gpu_busy_percent",
    "format": "Ôíº {}%",
    "return-type": "",
    "interval": 5
  },

  "cpu": {
    "interval": 5,
    "tooltip": false,
    "format": "Ôãõ  {usage}%",
    "format-alt": "Ôãõ {load}",
    "states": {
      "warning": 70,
      "critical": 90
    }
  },

  "temperature#cpu": {
    "critical-threshold": 90,
    "interval": 5,
    "hwmon-path": "/sys/class/hwmon/hwmon4/temp1_input",
    "format": "{icon} {temperatureC}¬∞C",
    "format-icons": [
      "Ôãã",
      "Ôãä",
      "Ôãâ",
      "Ôãà",
      "Ôãá"
    ],
    "tooltip": false
  },

  "temperature#gpu": {
    "critical-threshold": 90,
    "hwmon-path": "hwmon1",
    "hwmon-path": "/sys/class/hwmon/hwmon1/temp1_input",
    "interval": 5,
    "format": "{icon} {temperatureC}¬∞C",
    "format-icons": [
      "Ôãã",
      "Ôãä",
      "Ôãâ",
      "Ôãà",
      "Ôãá"
    ],
    "tooltip": false
  },

  "memory": {
    "interval": 5,
    "format": "Óâ¶  {used:0.1f}G/{total:0.1f}G",
    "states": {
      "warning": 70,
      "critical": 90
    },
    "tooltip": false
  },

  "network": {
    "interval": 5,
    "format-wifi": "Ôá´  {essid} ({signalStrength}%)",
    "format-ethernet": "{ifname}",
    "format-disconnected": "No connection",
    "format-alt": "{ipaddr}/{cidr}",
    "tooltip": false
  },

  // FIXME: Add BT codec
  "pulseaudio": {
    "format": "{icon} {desc} {volume}%",
    // "format-bluetooth": "{icon} {volume}%",
    "format-muted": "ÔÄ¶",
    "format-icons": {
      "headphone": "ÔÄ•",
      "hands-free": "ÔÄ•",
      "headset": "ÔÄ•",
      "phone": "ÔÇï",
      "portable": "ÔÇï",
      "car": "ÔÜπ",
      "default": [
        "ÔÄß",
        "ÔÄ®"
      ]
    },
    "scroll-step": 1,
    "on-click": "pactl set-sink-mute @DEFAULT_SINK@ toggle",
    "tooltip": false
  },

  // signal=2
  "custom/mus": { "exec": "~/.local/bin/i3blocks/mus", "interval": "5", },

  "custom/accuweather": { "exec": "~/.local/bin/i3blocks/accuweather", "interval": "1800", },
  "custom/distcompute": { "exec": "~/.local/bin/i3blocks/distcompute", "interval": "15", },
  "custom/cpu": { "exec": "~/.local/bin/i3blocks/cpu", "interval": "5", },
  "custom/dsk": { "exec": "~/.local/bin/i3blocks/dsk", "interval": "30", },
  "custom/vpn": { "exec": "~/.local/bin/i3blocks/vpn", "interval": "30", },
  // "custom/kb": { "exec": "~/.local/bin/i3blocks/kb", "interval": "100000000000", },

  // signal=13 
  "custom/vol": { "exec": "~/.local/bin/i3blocks/vol", "interval": "10", },

  "custom/bt": { "exec": "~/.local/bin/i3blocks/bt", "interval": "10", },
  "custom/bat": { "exec": "~/.local/bin/i3blocks/bat", "interval": "5", },
  "custom/time": { "exec": "~/.local/bin/i3blocks/time", "interval": "1", },

  // Separators
  "custom/blackf": { "format": "ÓÇ∫", "tooltip": false }, "custom/blackb": { "format": "ÓÇ∫", "tooltip": false },
  "custom/redf": { "format": "ÓÇ∫", "tooltip": false }, "custom/redb": { "format": "ÓÇ∫", "tooltip": false },
  "custom/greenf": { "format": "ÓÇ∫", "tooltip": false }, "custom/greenb": { "format": "ÓÇ∫", "tooltip": false },
  "custom/yellowf": { "format": "ÓÇ∫", "tooltip": false }, "custom/yellowb": { "format": "ÓÇ∫", "tooltip": false },
  "custom/bluef": { "format": "ÓÇ∫", "tooltip": false }, "custom/blueb": { "format": "ÓÇ∫", "tooltip": false },
  "custom/purplef": { "format": "ÓÇ∫", "tooltip": false }, "custom/purpleb": { "format": "ÓÇ∫", "tooltip": false },
  "custom/tealf": { "format": "ÓÇ∫", "tooltip": false }, "custom/tealb": { "format": "ÓÇ∫", "tooltip": false },
  "custom/grayf": { "format": "ÓÇ∫", "tooltip": false }, "custom/grayb": { "format": "ÓÇ∫", "tooltip": false },

  "custom/darkgrayf": { "format": "ÓÇ∫", "tooltip": false }, "custom/darkgrayb": { "format": "ÓÇ∫", "tooltip": false },
  "custom/brightredf": { "format": "ÓÇ∫", "tooltip": false }, "custom/brightredb": { "format": "ÓÇ∫", "tooltip": false },
  "custom/brightgreenf": { "format": "ÓÇ∫", "tooltip": false }, "custom/brightgreenb": { "format": "ÓÇ∫", "tooltip": false },
  "custom/brightyellowf": { "format": "ÓÇ∫", "tooltip": false }, "custom/brightyellowb": { "format": "ÓÇ∫", "tooltip": false },
  "custom/brightbluef": { "format": "ÓÇ∫", "tooltip": false }, "custom/brightbluef": { "format": "ÓÇ∫", "tooltip": false },
  "custom/magentaf": { "format": "ÓÇ∫", "tooltip": false }, "custom/magentab": { "format": "ÓÇ∫", "tooltip": false },
  "custom/cyanf": { "format": "ÓÇ∫", "tooltip": false }, "custom/cyanb": { "format": "ÓÇ∫", "tooltip": false },
  "custom/whitef": { "format": "ÓÇ∫", "tooltip": false }, "custom/whiteb": { "format": "ÓÇ∫", "tooltip": false },
}
